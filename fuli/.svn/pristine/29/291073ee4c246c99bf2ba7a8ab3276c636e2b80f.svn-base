package net.loyin.util.wechat.bean.mo;

/**
 * 账单查询：总计账单结构
 * 
 * @author EJ-Mr.Von
 */
public class QueryBillSum {
	private double amount;
	private double fee;
	private double preconcertPay;
	private double pay;

	public double getAmount() {
		return amount;
	}

	public void setAmount(double amount) {
		this.amount = amount;
	}

	public double getFee() {
		return fee;
	}

	public void setFee(double fee) {
		this.fee = fee;
	}

	public double getPreconcertPay() {
		return preconcertPay;
	}

	public void setPreconcertPay(double preconcertPay) {
		this.preconcertPay = preconcertPay;
	}

	public double getPay() {
		return pay;
	}

	public void setPay(double pay) {
		this.pay = pay;
	}
}
