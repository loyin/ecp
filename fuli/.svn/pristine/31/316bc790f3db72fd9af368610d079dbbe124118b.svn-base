<#include "../inc/layout.html"> <@layout title="用户">
<div class="nav">
	<ul class="cc">
		<li class="current"><a href="${root!}/jbmember/listl">帮会成员</a></li>
	</ul>
</div>
<div class="common-form">
	<form method="post" class="J_ajaxForm" action="#">
		<div class="table_list">
			<table width="100%">
				<thead>
					<tr>
						<td width="50">ID</td>
						<td>姓名</td>
						<td>加入时间</td>
						<td>酒帮ID</td>
						<td>微信用户id</td>
						<td>是否帮主</td>
					</tr>
				</thead>
				<tbody>
					<#assign status=['
				<font color="red">×</font>','
				<font color="green">√</font>']>
					<#list page.list as u>
					<tr>
						<td align='center'>${(u.id)!}</td>
						<td>${(u.uname)!}</td>
						<td>${(u.jrtime)!}</td>
						<td>${(u.jbid)!}</td>
						<td>${(u.wxid)!}</td>
						<td>${status[(u.status)!0]}</td>
					</#list>
				</tbody>
			</table>
			<div class="p10"><@paginate
				action="/jbmember/list" pageNo=pageNo totalPage=page.totalPage!0
				totalCount=page.totalRow!0/></div>
		</div>
	</form>
</div>
</@layout>
